<div id="commission_targets_graph"
  class="flex flex-col h-full col-span-12 p-6 bg-white border rounded-lg shadow-sm lg:col-span-7 md:col-span-6 border-slate-200 dark:bg-slate-800 dark:border-none dark:text-slate-300"
  data-controller="dashboards--charts"
  data-dashboards--charts-team-goals-value="<%= (@team_goals || {}).to_json %>">
  <div class="flex items-start justify-between pb-6">
    <div class="flex-1">
      <h4 class="m-0 text-xl font-medium tracking-tight text-slate-800 dark:text-slate-300">Proyección de ventas vs metas (evolución diaria)</h4>
    </div>
  </div>
  <div class="relative flex-grow">
    <div class="absolute inset-0" data-dashboards--charts-target="teamGoals"></div>
  </div>
</div>