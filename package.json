{
  "name": "app",
  "private": "true",
  "scripts": {
    "build:css": "tailwindcss --postcss -i ./app/assets/stylesheets/application.tailwind.css -o ./app/assets/builds/application.css --minify",
    "build:css:prod": "tailwindcss --postcss -i ./app/assets/stylesheets/application.tailwind.css -o ./app/assets/builds/application.css --minify",
    "build": "esbuild app/javascript/*.* --bundle --sourcemap --outdir=app/assets/builds --public-path=assets",
    "build:js:prod": "esbuild app/javascript/*.* --bundle --minify --outdir=app/assets/builds --public-path=assets"
  },
  "dependencies": {
    "@hotwired/stimulus": "^3.2.2",
    "@hotwired/turbo-rails": "^8.0.5",
    "@rails/actiontext": "^7.2.0",
    "@rails/request.js": "^0.0.9",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.14",
    "@uppy/compressor": "^2.0.1",
    "@uppy/locales": "^4.0.3",
    "apexcharts": "^3.53.0",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.4",
    "datatables.net": "^2.1.3",
    "datatables.net-buttons": "^3.1.1",
    "datatables.net-fixedheader": "^4.0.1",
    "datatables.net-responsive": "^3.0.2",
    "datatables.net-select": "^2.0.4",
    "esbuild": "^0.23.0",
    "jquery": "^3.7.1",
    "moment": "^2.30.1",
    "pluralize": "^8.0.0",
    "postcss": "^8.4.41",
    "postcss-import": "^16.1.0",
    "postcss-nesting": "^13.0.0",
    "stimulus-use": "^0.52.2",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss": "^3.4.9",
    "tippy.js": "^6.3.7",
    "tom-select": "^2.3.1",
    "trix": "^2.1.5",
    "uppy": "3.27.4"
  },
  "resolutions": {
    "micromatch": "4.0.8"
  }
}
